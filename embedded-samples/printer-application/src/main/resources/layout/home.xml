<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="960dp"
        android:layout_height="640dp">

    <ImageView
            android:id="@+id/scan"
            android:layout_width="960dp"
            android:layout_height="640dp"
            android:background="@drawable/scan"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp" />

    <ImageButton
            android:id="@+id/ImageButton"
            android:layout_width="70dp"
            android:layout_height="70dp"
            tools:layout_editor_absoluteX="830dp"
            tools:layout_editor_absoluteY="511dp"
            android:background="@drawable/home_config" />

    <ImageButton
            android:id="@+id/ImageButton3"
            android:layout_width="70dp"
            android:layout_height="70dp"
            tools:layout_editor_absoluteX="730dp"
            tools:layout_editor_absoluteY="511dp"
            android:background="@drawable/home_wifi" />

    <Button
            android:id="@+id/bprint"
            android:layout_width="243dp"
            android:layout_height="260dp"
            tools:layout_editor_absoluteX="358dp"
            tools:layout_editor_absoluteY="188dp"
            android:background="@drawable/PRINT" />

    <Button
            android:id="@+id/bscan"
            android:layout_width="242dp"
            android:layout_height="262dp"
            tools:layout_editor_absoluteX="57dp"
            tools:layout_editor_absoluteY="188dp"
            android:background="@drawable/BSCAN" />

    <Button
            android:id="@+id/bcopy"
            android:layout_width="246dp"
            android:layout_height="264dp"
            tools:layout_editor_absoluteX="657dp"
            tools:layout_editor_absoluteY="187dp"
            android:background="@drawable/COPY" />

    <TextView
            android:id="@+id/TextView"
            android:layout_width="70dp"
            android:layout_height="26dp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:gravity="center"
            android:textSize="20dp"
            tools:layout_editor_absoluteX="628dp"
            tools:layout_editor_absoluteY="81dp"
            android:text="2:45PM" />

    <TextView
            android:id="@+id/textView"
            android:layout_width="132dp"
            android:layout_height="26dp"
            android:text="Tue, 12/08/20"
            android:textColor="#000000"
            android:textStyle="bold"
            android:gravity="center"
            android:textSize="20dp"

            tools:layout_editor_absoluteX="752dp"
            tools:layout_editor_absoluteY="81dp" />
    <ImageView
            android:id="@+id/Image9"
            android:layout_width="305dp"
            android:layout_height="70dp"
            android:background="@drawable/home_fundohorario_data"
            tools:layout_editor_absoluteX="600dp"
            tools:layout_editor_absoluteY="59dp" />


    <TextView
            android:id="@+id/TextView3"
            android:layout_width="288dp"
            android:layout_height="111dp"
            android:text="Hello! What do you need to do?"
            android:textColor="#000000"
            android:textSize="36dp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="48dp"
            tools:layout_editor_absoluteY="50dp" />

    <ImageView
            android:id="@+id/ImageView3"
            android:layout_width="326dp"
            android:layout_height="73dp"
            tools:layout_editor_absoluteX="62dp"
            tools:layout_editor_absoluteY="507dp"
            android:background="@drawable/LEVEL" />


</androidx.constraintlayout.widget.ConstraintLayout>