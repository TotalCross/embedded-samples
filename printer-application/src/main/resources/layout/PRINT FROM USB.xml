<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="960dp"
        android:layout_height="640dp">
    <ImageView
            android:id="@+id/scan"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp"
            android:layout_width="960dp"
            android:layout_height="640dp"
            android:background="@drawable/scan"
    />
    <ImageView
            android:id="@+id/ImageView"
            android:layout_width="844dp"
            android:layout_height="283dp"
            tools:layout_editor_absoluteX="57dp"
            tools:layout_editor_absoluteY="298dp"
            android:background="@drawable/retangulo" />

    <Button
            android:id="@+id/back"
            android:layout_width="36dp"
            android:layout_height="46dp"
            android:background="@drawable/home_seta"
            tools:layout_editor_absoluteX="85dp"
            tools:layout_editor_absoluteY="55dp" />
    <Button
            android:id="@+id/home"
            android:layout_width="41dp"
            android:layout_height="40dp"
            tools:layout_editor_absoluteX="858dp"
            tools:layout_editor_absoluteY="57dp"
            android:background="@drawable/home" />
    <TextView
            android:id="@+id/TextView"
            android:layout_width="498dp"
            android:layout_height="53dp"
            android:textStyle="bold"
            android:textSize="36dp"
            android:textColor="#000000"
            android:gravity="center"
            tools:layout_editor_absoluteX="230dp"
            tools:layout_editor_absoluteY="174dp"
            android:text="Please select a file to print." />

    <Button
            android:id="@+id/print"
            android:layout_width="252dp"
            android:layout_height="53dp"
            android:background="@drawable/printbutton"
            tools:layout_editor_absoluteX="121dp"
            tools:layout_editor_absoluteY="343dp" />

    <TextView
            android:id="@+id/TextView3"
            android:layout_width="434dp"
            android:layout_height="62dp"
            android:textStyle="bold"
            android:textSize="48dp"
            android:textColor="#2B05C7"
            android:gravity="center"

            tools:layout_editor_absoluteX="264dp"
            tools:layout_editor_absoluteY="45dp"
            android:text="PRINT FROM USB" />



</androidx.constraintlayout.widget.ConstraintLayout>